{
  "name": "file-existence-action",
  "version": "3.0.0",
  "private": true,
  "description": "GitHub Action to check for file existence",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "mocha",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andstor/file-existence-action.git"
  },
  "keywords": [
    "actions",
    "file",
    "existence",
    "checker"
  ],
  "author": {
    "name": "Andr√© Storhaug",
    "email": "andr3.storhaug@gmail.com",
    "url": "https://github.com/andstor"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andstor/file-existence-action/issues"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "glob": "^7.2.3",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.11.7",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/parser": "^5.41.0",
    "@vercel/ncc": "^0.38.1",
    "chai": "^4.3.6",
    "eslint": "^8.26.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^26.9.0",
    "js-yaml": "^4.1.0",
    "mocha": "^9.2.2",
    "prettier": "2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
